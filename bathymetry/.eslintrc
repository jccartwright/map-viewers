{
    "env": {
        "browser": true,
        "amd": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "warn",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console":[ "warn"]
    },
    "globals": {
        //"define": true,
        "Wkt": true,  //provided by Wicket
        "Proj4js": true,
        "logger": true
    }
}
